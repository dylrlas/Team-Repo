function _init()
 sfx(1)
 drawlogo()
end

--basic game logo on the splash screen
--create game dev. logo

fcount = 0

logoanim=1
logostop=31
logofr=64

function drawlogo()
 local c = 0
 local cx = 64
 local cy = 68
 local frx=logofr%16*8
 local fry=flr(logofr/16)*8
 for s=0,logoanim do
  for x=0,15 do
  if(c==15) then
		 	spr(64, 68, 81, 2, 2)
		end
   camera(rnd(30/fcount),
          rnd(30/fcount))
   for y=0,15 do
    if(x+y==s) then 
     pset(cx+x,cy+y,7)
    elseif(x+y == s-1) then
     pset(cx+x,cy+y,6)
    elseif(x+y<s-1) then
     pset(cx+x,cy+y,
      sget(frx+x,fry+y))
    end
   end
  end
 end
 if(logoanim<=logostop) logoanim+=1
 if(fcount > 35) then
  print("radical",51,cy+20,8)
  print("ascension",55,cy+26,8)
 end
end

function _update()
 fcount += 1
end

function _draw()
 cls()
 drawlogo()
end
